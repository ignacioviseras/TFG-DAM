package com.Edix.FinalProyect.com.Edix.FinalProyect.beans;

public class Admin {
	private int id;
	private String name;
	private String password;
	private Access access;
	private String token;
	
	//create a empty constructor
	
	public Admin() {
		
	}

	//create a constructor with properties
	
	public Admin(int id, String name, String password, Access access) {
		super();
		this.id = id;
		this.name = name;
		this.password = password;
		this.access = access;
	}
	
	//create a getters and setters to have access private properties

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public Access getAccess() {
		return access;
	}

	public void setAccess(Access access) {
		this.access = access;
	}

	//add a ToString
	
	@Override
	public String toString() {
		return "Admin [id=" + id + ", name=" + name + ", password=" + password + ", access=" + access + "]";
	}
	
	
	
	
	
	
	

}
